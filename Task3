import random
x = int(input())
def bubble_sort(row):
    for i in range(len(row) + 1):
        for j in range(len(row) - 1 - i):
            if row[j] > row[j + 1]:
                row[j], row[j + 1] = row[j + 1], row[j]
    return row
array = [[random.randint(0, 10) for i in range(x)] for j in range(x)]
print("Исходная матрица:")
for rowss in array:
    print(rowss)
shifts = 1
for x, row in enumerate(array):
    array[x] = bubble_sort(row)
    print("Строка после сортировки:", array[x])
