import random
x = int(input())
def shift_rows(matrix, shifts):
    rows = len(matrix)
    shifts = shifts % rows
    return matrix[-shifts:] + matrix[:-shifts]
array = [[random.randint(0, 10) for i in range(x)] for j in range(x)]
print("Исходная матрица:")
for rowss in array:
    print(rowss)
print()
shifts = 1
result = shift_rows(array, shifts)
print("Матрица после сдвига:")
for i, row in enumerate(result):
    print(row)
